{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "paths": {
        "hud:*": ["app/*"]
    },
    "exclude": [
        "node_modules",
        "jspm_packages"
    ]
  }
}
